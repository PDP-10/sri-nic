;Documentation for product
COPY FROM-SOURCE:ACTGEN.HLP TO-SUBSYS:*.*.-1
COPY FROM-SOURCE:ACTGEN.DOC TO-DOC:*.*.-1

;Final product
COPY FROM-SOURCE:ACTGEN.EXE TO-SUBSYS:*.*.-1
